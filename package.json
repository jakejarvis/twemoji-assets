{
  "name": "twemoji-assets",
  "version": "13.0.1",
  "description": "A very simple downloader for Twemoji graphics (PNG and SVG)",
  "license": "CC-BY-4.0",
  "repository": "jakejarvis/twemoji-assets",
  "author": {
    "name": "Jake Jarvis",
    "email": "jake@jarv.is",
    "url": "https://jarv.is/"
  },
  "keywords": [
    "twemoji",
    "emoji",
    "vector",
    "svg",
    "front-end"
  ],
  "scripts": {
    "preinstall": "del-cli vendor",
    "postinstall": "download --extract --strip 2 --out vendor https://github.com/twitter/twemoji/archive/v13.0.1.zip && del-cli \"vendor/**\" \"vendor/.*\" \"!vendor/72x72\" \"!vendor/svg\""
  },
  "dependencies": {
    "del-cli": "^3.x",
    "download-cli": "^1.x"
  }
}
